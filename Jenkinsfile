pipeline {
  agent any
  stages {
    stage('Fluffy Build') {
      steps {
        echo 'i am batman build'
      }
    }

    stage('Fluffy Test') {
      steps {
        echo 'i am batman test'
      }
    }

    stage('Fluffy Deploy') {
      steps {
        echo 'i am batman deploy'
      }
    }

  }
}